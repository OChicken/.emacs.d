#+begin_src gnuplot :session :file /tmp/gnuplot.png :exports both
set term png size 400,300  # or qt, postscript, epslatex
set title "The title"
set xlabel "x"
set ylabel "y"
set xrange [-pi:pi]
set yrange [-1:1]
# set xtics -10:0.1:10
# set ytics -1,0.1,1
$0
#+end_src
