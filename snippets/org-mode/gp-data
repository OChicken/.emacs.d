# -*- mode: snippet -*-
# name: gnuplot-data
# key:  gp-data
# --
#+begin_src gnuplot :var data=\"data.csv\" :file /tmp/gnuplot.png :exports both
set datafile separator ","
set term png size 400,300  # or qt, postscript, epslatex
set title "The title"
set xlabel "x"
set ylabel "y"
set zeroaxis
set grid
set xrange [-pi:pi]
set yrange [-1:1]
plot data using 1:2 with linespoints linewidth 1 title "1st sample",\
     data using 1:3 with linespoints linewidth 1 title "2nd sample"
$0
#+end_src
